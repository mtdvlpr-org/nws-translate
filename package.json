{
  "name": "nws-translate",
  "description": "Een tool om New World Scheduler te vertalen.",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare:husky": "node .husky/install.mjs",
    "prepare:nuxt": "nuxt prepare",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:types": "nuxt typecheck",
    "test": "vitest run",
    "test:watch": "vitest --watch --coverage",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest --project unit",
    "test:nuxt": "vitest --project nuxt",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "postinstall": "pnpm prepare:husky && pnpm prepare:nuxt"
  },
  "dependencies": {
    "@fastify/busboy": "^3.2.0",
    "@iconify-json/lucide": "^1.2.91",
    "@iconify-json/simple-icons": "^1.2.71",
    "@nuxt/ui": "^4.4.0",
    "@pinia/nuxt": "^0.11.3",
    "@vueuse/nuxt": "^14.2.1",
    "@vueuse/router": "^14.2.1",
    "jszip": "^3.10.1",
    "node-html-parser": "^7.0.2",
    "nuxt": "^4.3.1",
    "pako": "^2.1.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "sql.js": "^1.14.0",
    "tailwindcss": "^4.2.0",
    "unzipper": "^0.12.3",
    "v-code-diff": "^1.13.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.15.1",
    "@nuxt/test-utils": "^4.0.0",
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "^1.58.2",
    "@types/eslint-plugin-security": "^3.0.1",
    "@types/node": "^22.19.11",
    "@types/pako": "^2.0.4",
    "@types/sql.js": "^1.4.9",
    "@types/unzipper": "^0.10.11",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^5.6.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-security": "^3.0.1",
    "happy-dom": "^20.6.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "playwright-core": "^1.58.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-static-copy": "^3.2.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "engines": {
    "node": "^22.18.0",
    "pnpm": "^10.30.0"
  },
  "packageManager": "pnpm@10.30.0"
}
